{
  "name": "fe2cm-audio-player",
  "productName": "FE2CM Audio Player",
  "version": "1.2.2",
  "description": "FE2CM Audio Player",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "myralune",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.fe2cm.audioplayer",
    "productName": "FE2CM Audio Player",
    "publish": [
      {
        "provider": "github",
        "owner": "myralune",
        "repo": "FE2CM-Audio-Player"
      }
    ],
    "asarUnpack": [
      "node_modules/uiohook-napi/**"
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "artifactName": "FE2CMAudioPlayer-Setup-${version}.exe"
    }
  },
  "dependencies": {
    "electron-updater": "^6.7.3",
    "uiohook-napi": "^1.5.4"
  }
}
